{
	"name": "itsu",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"engines": {
		"node": "15.x"
	},
	"scripts": {
		"start": "node index.js",
		"start-dev": "babel-node index.js",
		"build": "cd client && npm run build",
		"clean": "rm -rf build && mkdir build",
		"dev": "cross-env NODE_ENV=development nodemon --ignore ./client/ --exec 'npx babel-node index.js'",
		"start2": "npm-run-all -p -r dev start:client",
		"alias": "node alias-hq",
		"build:server": "babel-node index.js",
		"format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
		"heroku-prebuild": "cd client && npm install"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/restify-errors": "^4.3.3",
		"alias-hq": "^5.1.5",
		"axios": "^0.19.2",
		"babel-cli": "7.0.0-beta.3",
		"bunyan": "^1.8.15",
		"ccxt": "^1.49.73",
		"config": "^3.3.1",
		"cors": "^2.8.5",
		"dotenv": "^9.0.0",
		"express": "^4.17.1",
		"express-async-handler": "^1.1.4",
		"intervals-for-humans": "^1.0.4",
		"joi": "^17.2.1",
		"memory-cache": "^0.2.0",
		"restify-errors": "^8.0.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.13.16",
		"@babel/core": "^7.14.0",
		"@babel/node": "^7.13.13",
		"@babel/preset-env": "^7.14.1",
		"cross-env": "^7.0.2",
		"eslint": "^7.25.0",
		"eslint-config-prettier": "^7.2.0",
		"prettier": "^2.2.1"
	},
	"alias-hq": {
		"folders": [
			"."
		],
		"modules": [
			"models",
			"controllers",
			"config",
			"middleware",
			"utils"
		]
	}
}
